struct Lexer {
	stream: str,
	index: num,
	filename: str,
	lineno: num
}

enum TokenKind {
	EOF{},
	Number { n: num },
	String { s: str },
	Identifier { i: str },
	Symbol { s: str },
}

struct StreamPos {
	lineno: num,
	filename: str
}

struct Token {
	kind: TokenKind,
	pos: StreamPos
}

externf Lexer_new: fn(str, str): Lexer
externf Lexer.pos: fn(Lexer): StreamPos
externf Lexer.parse: fn(Lexer): Token
externf StreamPos.to_str: fn(StreamPos): str
externf TokenKind.inspect: fn(TokenKind): str
